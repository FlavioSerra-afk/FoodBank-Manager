# === FoodBank Manager (local/dev) ===
# Copy to .env (do NOT commit .env), and map into wp-config.php if you wish.
# KEK must be base64-encoded 32+ bytes (DEMO ONLY below).
FBM_KEK_BASE64=bXktZGV2LXN1cGVyLXNlY3JldC1rZWstMzJieXRlcy1iYXNlNjQ=

# Email defaults (optional; your site may use SMTP plugins instead)
FBM_FROM_NAME="FoodBank"
FBM_FROM_EMAIL="no-reply@example.test"

# CAPTCHA (optional)
RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET=
TURNSTILE_SITE_KEY=
TURNSTILE_SECRET=

# WP environment hints
WP_ENVIRONMENT_TYPE=local

